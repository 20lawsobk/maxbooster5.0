modules = ["nodejs-20", "bash", "web"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["libuuid", "cairo", "pango", "libpng", "libjpeg", "giflib", "pkg-config", "pixman"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Max Booster Platform"

[[workflows.workflow]]
name = "Max Booster Platform"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd Max-Booster && npm run dev"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

