modules = ["nodejs-20", "bash", "web", "postgresql-16"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["libuuid", "cairo", "pango", "libpng", "libjpeg", "giflib", "pkg-config", "pixman", "redis", "jq"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Max Booster Platform"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Burn-in Test (6-Hour)"

[[workflows.workflow]]
name = "Max Booster Platform"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd Max-Booster && ./start-with-redis.sh"
waitForPort = 5000

[[workflows.workflow]]
name = "Burn-in Test (6-Hour)"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd Max-Booster && npm run test:burn-in"

[workflows.workflow.metadata]
